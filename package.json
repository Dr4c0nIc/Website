{
  "name": "fyre-website",
  "version": "3.0.0",
  "main": "src/index",
  "license": "MIT",
  "scripts": {
    "start": "node --trace-warnings dist/index",
    "compile": "yarn --silent && rimraf dist && tsc && yarn copyFiles",
    "copyFiles": "copyfiles -f src/styles/* dist/styles && copyfiles -f src/assets/* dist/assets",
    "quick": "yarn run eslint src --fix --quiet && yarn compile && yarn start"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-oauth2": "^1.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jquery": "^3.5.5",
    "@types/node": "^14.14.33",
    "@types/passport": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.23.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.3"
  }
}
